{
  "ConnectionStrings": {
    "ConversationDbContext": "Host=postgres-conversation;Port=5432;Database=conversation;Username=postgres;Password=postgres"
  },
  "RabbitMQ": {
    "Host": "rabbitmq",
    "Port": 5672,
    "VirtualHost": "/",
    "Username": "guest",
    "Password": "guest"
  },
  "Keycloak": {
    "Url": "http://keycloak:8080",
    "Realm": "petfamily",
    "ClientId": "petfamily-backend",
    "ClientSecret": ""
  },
  "Caching": {
    "Type": "Redis",
    "DefaultExpiration": "00:05:00",
    "KeyPrefix": "conversation",
    "RedisConnectionString": "redis:6379"
  },
  "Elasticsearch": {
    "Url": "http://elasticsearch:9200",
    "IndexFormat": "petfamily-conversation-api-{0:yyyy.MM}"
  }
}

{
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:6000",
    "RequestIdKey": "OcsRequestId",
    "RateLimitOptions": {
      "ClientIdHeader": "ClientId",
      "QuotaExceededMessage": "Too many requests. Please try again later.",

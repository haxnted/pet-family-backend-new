{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MinIO": {
    "Endpoint": "localhost:9000",
    "AccessKey": "minioadmin",
    "SecretKey": "minioadmin",
    "BucketName": "petfamily-files",
    "Secure": false,
    "PresignedUrlExpiryInSeconds": 3600
  },
  "RabbitMQ": {
    "Host": "localhost",
    "Port": 5672,
    "VirtualHost": "/",
    "Username": "guest",
    "Password": "guest"
  },
  "Keycloak": {
    "Url": "http://localhost:8080",
    "Realm": "petfamily",
    "ClientId": "petfamily-backend",
    "ClientSecret": "etaya"
  },
  "Elasticsearch": {
    "Url": "http://localhost:9200",
    "IndexFormat": "petfamily-filestorage-api-{0:yyyy.MM}"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    }
  }
}
